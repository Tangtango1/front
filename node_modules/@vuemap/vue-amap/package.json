  "bugs": {
    "url": "https://gitee.com/guyangyang/vue-amap/issues"
  },
  "homepage": "https://vue-amap.guyixi.cn",
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@vuemap/vue-amap-util": "2.0.1",
    "@vuemap/district-cluster": "^0.0.4"
  },
  "devDependencies": {
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "vetur": {
    "tags": "./ide/tags.json",
    "attributes": "./ide/attributes.json"
  },
  "web-types": "./ide/web-types.json"
