      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    },
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.js",
    "./*": "./*"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "style": "dist/style.css",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "sideEffects": [
    "dist/*",
    "lib/packages/**/*",
    "es/packages/**/*"
  ],
